{
  "name": "boilerplate",
  "version": "1.0.0",
  "author": "Infinum",
  "private": true,
  "main": "",
  "scripts": {
    "__sassUnusedTheme": "sass-unused \"wp-content/themes/theme_name/**/*.scss\"",
    "__eslintTheme": "eslint wp-content/themes/theme_name/skin/assets/",
    "__stylelintTheme": "stylelint \"wp-content/themes/theme_name/**/*.scss\" --syntax scss",
    "precommit": "npm run __eslintTheme && npm run __stylelintTheme && npm run __sassUnusedTheme",
    "start": "webpack --progress --watch --display-error-details --display-modules --display-reasons",
    "build": "NODE_ENV=production webpack --progress"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "bugsnag-js": "^3.2.1",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.5",
    "css-mqpacker": "^6.0.1",
    "cssnano": "^3.10.0",
    "eslint": "^4.5.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.3",
    "postcss-cssnext": "^3.0.2",
    "postcss-font-magician": "^2.0.0",
    "postcss-loader": "^2.0.6",
    "precss": "^2.0.0",
    "sass-loader": "^6.0.6",
    "sass-unused": "^0.2.2",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.9.0"
  },
  "dependencies": {
    "jquery": "^3.2.1",
    "media-blender": "^2.1.0",
    "normalize.css": "^7.0.0",
    "slick-carousel": "^1.7.1",
    "susy": "^3.0.1",
    "jquery-match-height": "^0.7.2"
  },
  "browserslist": [
    "android >= 4.2",
    "not ie < 11",
    "last 2 versions"
  ]
}
